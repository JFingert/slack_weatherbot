{
  "name": "weatherbot",
  "version": "1.0.5",
  "description": "a Forecast.io Slack bot.",
  "keywords": [
    "Slack",
    "bot",
    "Forecast.io",
    "weather",
    "WeatherBot"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/"
  },
  "main": "lib/weatherbot.js",
  "bin": {
    "weatherbot": "bin/bot.js"
  },
  "scripts": {
    "start": "node bin/bot.js",
    "test": "node_modules/.bin/xo bin/bot.js lib/weatherbot.js"
  },
  "author": {
    "name": "Joshua Fingert",
    "email": "joshuafingert@gmail.com"
  },
  "license": "MIT",
  "engines": {
    "node": ">=0.10.0"
  },
  "dependencies": {
    "async": "^1.4.1",
    "progress": "^1.1.8",
    "request": "^2.74.0",
    "slackbots": "^0.3.0"
  },
  "devDependencies": {
    "xo": "^0.5.2"
  },
  "xo": {
    "space": 4,
    "rules": {
      "camelcase": [
        2,
        {
          "properties": "never"
        }
      ]
    }
  }
}
